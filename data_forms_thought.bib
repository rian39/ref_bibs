  date = {2015-08-01},
  pages = {395--412},
  keywords = {Algorism,algorithm,algorithmic culture,big data,crowd,Culture,Information,keywords,Raymond Williams},
  file = {Full Text PDF:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/WHS982A9/Striphas - 2015 - Algorithmic culture.pdf:application/pdf;Snapshot:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/3MP75CWQ/395.html:text/html},
