  pages = {115--153},
  keywords = {Artificial Intelligence (incl. Robotics),cross-sell,customer loyalty,database marketing,Data Mining,Data Structures; Cryptology and Information Theory,Electronic commerce,Information Storage and Retrieval,mass customization,personalization,privacy,recommender systems,Statistics for Engineering; Physics; Computer Science; Chemistry & Geosciences,Statistics; general,up-sell,user interface}
}

