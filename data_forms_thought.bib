  pages = {341--366},
  file = {Full Text PDF:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/RP6CXWJI/Amoore and Piotukh - 2015 - Life beyond big data governing with little analyt.pdf:application/pdf;Snapshot:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/2ZQR2BVT/03085147.2015.html:text/html},
