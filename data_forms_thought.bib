  author = {Koren, Yehuda and Bell, Robert and Volinsky, Chris},
  date = {2009},
  file = {Citeseer - Full Text PDF:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/KSM4IF25/Koren et al. - 2009 - Matrix Factorization Techniques for Recommender Sy.pdf:application/pdf;Citeseer - Snapshot:/home/mackenza/.mozilla/firefox/eudyvw4k.default-1491868731243/zotero/storage/RHNCXHWT/summary.html:text/html},
