  langid = {english},
  publisher = {{Springer International Publishing}},
  author = {Kusch, Maren},
  date = {2012-12-06},
  keywords = {History / General,Reference / General,Science / Philosophy & Social Aspects,Social Science / Archaeology,Social Science / General},
  groups = {secondary\_sources}
}

