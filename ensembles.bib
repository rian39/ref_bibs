  file = {/home/mackenza/Zotero/storage/2FSDH4PX/1146.html}
}

@book{wachter-boettcherTechnicallyWrongSexist2017,
  title = {Technically {{Wrong}}: {{Sexist Apps}}, {{Biased Algorithms}}, and {{Other Threats}} of {{Toxic Tech}}},
