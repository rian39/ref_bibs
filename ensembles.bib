@inproceedings{nguyenDeepNeuralNetworks2015,
  title = {Deep Neural Networks Are Easily Fooled: {{High}} Confidence Predictions for Unrecognizable Images},
  shorttitle = {Deep Neural Networks Are Easily Fooled},
