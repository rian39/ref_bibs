@article{Gunnarsdottir_2005,
  title = {Scientific {{Journal Publications}}: {{On}} the {{Role}} of {{Electronic Preprint Exchange}} in the {{Distribution}} of                 {{Scientific Literature}}},
  shorttitle = {Scientific {{Journal Publications}}},
  author = {Gunnarsdóttir, Kristrún},
  date = {2005-08-01},
  journaltitle = {Social Studies of Science},
