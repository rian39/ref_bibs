  file = {/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/WLHXXDE2/Diggle - 2015 - Statistics a data science for the 21st century.pdf;/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/324LBJIL/abstract.html}
}

