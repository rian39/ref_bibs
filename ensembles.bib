  author = {Filev Maia, Rodrigo and Ballester Lurbe, Carlos and Agrahari Baniya, Arbind and Hornbuckle, John},
  date = {2020-12-14},
  journaltitle = {Sensors (Basel, Switzerland)},
  shortjournal = {Sensors (Basel)},
  volume = {20},
  number = {24},
  eprint = {33327512},
