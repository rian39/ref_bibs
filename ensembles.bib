@article{Helmond_2015,
  langid = {english},
  title = {The {{Platformization}} of the {{Web}}: {{Making Web Data Platform Ready}}},
  volume = {1},
  issn = {2056-3051},
  url = {https://doi.org/10.1177/2056305115603080},
