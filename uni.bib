@article{Hasinoff_2021,
  title = {Feature {{Analysis}}: {{A Method}} for {{Analyzing}} the {{Role}} of {{Ideology}} in {{App Design}}},
  shorttitle = {Feature {{Analysis}}},
  author = {Hasinoff, Amy and Bivens, Rena},
  date = {2021-09-02},
  journaltitle = {Journal of Digital Social Research},
  volume = {3},
