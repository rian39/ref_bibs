@article{Rose_2019,
  title = {Seeing the Smart City on {{Twitter}}: {{Colour}} and the Affective Territories of Becoming Smart},
  shorttitle = {Seeing the Smart City on {{Twitter}}},
  author = {Rose, Gillian and Willis, Alistair},
  date = {2019-06-01},
  journaltitle = {Environment and Planning D: Society and Space},
  shortjournal = {Environ Plan D},
  volume = {37},
