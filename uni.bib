  author = {Lecun, Y. and Bottou, L. and Bengio, Y. and Haffner, P.},
  file = {/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/M46VHRG7/Lecun et al. - 1998 - Gradient-based learning applied to document recogn.pdf;/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/A9D2NKIF/726791.html}
