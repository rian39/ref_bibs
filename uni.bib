  langid = {english},
  keywords = {Atmospheric dynamics,Climate and Earth system modelling},
  file = {/home/mackenza/Zotero/storage/2DWRINSN/Di Capua et al. - 2021 - Drivers behind the summer 2010 wave train leading .pdf;/home/mackenza/Zotero/storage/JBSBWHQV/s41612-021-00211-9.html}
}

