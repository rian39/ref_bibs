@article{Gordon_2019,
  title = {Grieving Environmental Scientists Need Support},
  author = {Gordon, Timothy A. C. and Radford, Andrew N. and Simpson, Stephen D.},
  date = {2019-10-11},
  journaltitle = {Science},
  volume = {366},
  number = {6462},
  eprint = {31601761},
  eprinttype = {pmid},
