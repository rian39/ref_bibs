  keywords = {species sorting,Mass effects,neutral model,metacommunity,patch dynamics},
  author = {Leibold, M. A. and Holyoak, M. and Mouquet, N. and Amarasekare, P. and Chase, J. M. and Hoopes, M. F. and Holt, R. D. and Shurin, J. B. and Law, R. and Tilman, D. and Loreau, M. and Gonzalez, A.},
  file = {/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/X76ST5XJ/Leibold et al. - 2004 - The metacommunity concept a framework for multi-s.pdf;/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/ADFRVUXJ/full.html}
