@article{Walls_2004,
  title = {Critical Trust: Understanding Lay Perceptions of Health and Safety Risk Regulation},
  volume = {6},
  doi = {10.1080/1369857042000219788},
  shorttitle = {Critical Trust},
  abstract = {The binary opposition of trusting or not trusting is inadequate to understand the often ambiguous and contradictory ideas people possess about risk regulators, particularly when knowledge and experience of such institutions is limited. The paper reports qualitative and quantitative data from a major study of public perceptions (n = 30 focus groups) of UK risk regulators. We compare the complex and widely different 'trust profiles' of two regulatory organisations which are institutionally related ( the Health and Safety Executive and the Railways Inspectorate) but very separate in the minds of our participants. The paper develops the notion of critical trust to interrogate the various ways in which people make sense of such organisations, as well as discussing the modes of reasoning that people deploy. The paper argues that views of participants are the outcome of a reconciliation of diverse perceptions concerning the role of the organisation, structural factors and the nature of the regulated risk.},
  number = {2},
  journaltitle = {Health Risk \& Society},
  date = {2004-06},
  pages = {133-150},
  author = {Walls, J. and Pidgeon, N. and Weyman, A. and Horlick-Jones, T.},
  note = {WOS:000221583800003}
}

@article{SainzdeMurieta_2012,
  title = {{{DNA}} Biosensors That Reason},
  volume = {109},
  issn = {0303-2647},
  url = {http://www.sciencedirect.com/science/article/pii/S0303264712000470},
  doi = {10.1016/j.biosystems.2012.02.005},
  abstract = {Despite the many designs of devices operating with the DNA strand displacement, surprisingly none is explicitly devoted to the implementation of logical deductions. The present article introduces a new model of biosensor device that uses nucleic acid strands to encode simple rules such as “IF DNA \_ strand1 is present THEN diseaseA” or “IF DNA \_ strand1 AND DNA \_ strand2 are present THEN diseaseB”. Taking advantage of the strand displacement operation, our model makes these simple rules interact with input signals (either DNA or any type of RNA) to generate an output signal (in the form of nucleotide strands). This output signal represents a diagnosis, which either can be measured using FRET techniques, cascaded as the input of another logical deduction with different rules, or even be a drug that is administered in response to a set of symptoms. The encoding introduces an implicit error cancellation mechanism, which increases the system scalability enabling longer inference cascades with a bounded and controllable signal–noise relation. It also allows the same rule to be used in forward inference or backward inference, providing the option of validly outputting negated propositions (e.g. “diagnosis A excluded”). The models presented in this paper can be used to implement smart logical DNA devices that perform genetic diagnosis in vitro.},
  number = {2},
  journaltitle = {Biosystems},
  shortjournal = {Biosystems},
  urldate = {2014-03-05},
  date = {2012-08},
  pages = {91-104},
  keywords = {DNA computing,In vitro diagnosis,Inference,Strand displacement,Biosensor,Biomolecular computation},
  author = {Sainz de Murieta, Iñaki and Rodríguez-Patón, Alfonso},
  file = {/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/2QRNJDRI/Sainz de Murieta and Rodríguez-Patón - 2012 - DNA biosensors that reason.pdf;/home/mackenza/.zotero/zotero/zl26h2qh.default/zotero/storage/F7F7DP37/S0303264712000470.html}
}

